    "babel-loader": "^8.0.4",
    "babel-preset-airbnb": "^3.2.1",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.5.7",
    "electron": "^3.0.4",
    "electron-packager": "^12.2.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.12.0",
    "react": "^16.5.2",
