    "babel-loader": "^8.0.4",
    "babel-preset-airbnb": "^3.2.1",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "electron": "^3.0.4",
    "electron-packager": "^12.2.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
