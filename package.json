{
  "name": "DotaStalk",
  "productName": "DotaStalk",
  "version": "0.7.0",
  "description": "Electron React React-Router boilerplate",
  "author": "BigFancyBen",
  "homepage": "https://github.com/BigFancyBen/dotastalk/releases",
  "repository": {
    "type": "git",
    "url": "https://github.com/BigFancyBen/dotastalk.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
